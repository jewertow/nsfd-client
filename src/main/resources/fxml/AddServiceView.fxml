<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="nsfd.desktop.controller.AddServiceController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
        <HBox alignment="CENTER">
            <padding><Insets top="10.0"/></padding>
            <Label text="Nazwa domeny" prefWidth="120.0"/>
            <TextField fx:id="domainNameInput"/>
        </HBox>
        <HBox alignment="CENTER">
            <padding><Insets top="10.0" bottom="10.0"/></padding>
            <Label text="Port" prefWidth="120.0"/>
            <TextField fx:id="portInput"/>
        </HBox>
        <Button fx:id="submit" text="Zapisz" alignment="CENTER"/>
    </VBox>
</AnchorPane>
